#include<bits/stdc++.h>
#include<iostream>
using namespace std;

int main()
{
    int n=3;
    int m=4;
    int i,j;
    
    vector<vector<int>>A(n,vector<int>(m));
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cin>>A[i][j];
        }
    }
    
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cout<<A[i][j];
        }
        cout<<"\n";
    }
    
    
}